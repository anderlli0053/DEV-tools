{
    "architecture": {
        "32bit": {
            "bin": "IDE\\bin\\phpstorm.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\phpstorm.exe",
                    "JetBrains\\PhpStorm"
                ]
            ]
        },
        "64bit": {
            "bin": [
                [
                    "IDE\\bin\\phpstorm64.exe",
                    "phpstorm"
                ]
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\phpstorm64.exe",
                    "JetBrains\\PhpStorm"
                ]
            ]
        }
    },
    "autoupdate": {
        "hash": {
            "url": "$url.sha256"
        },
        "url": "https://download.jetbrains.com/webide/PhpStorm-$matchHead.exe#/dl.7z"
    },
    "checkver": {
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}",
        "url": "https://data.services.jetbrains.com/products/releases?code=PS&latest=true&platform=zip&type=release"
    },
    "description": "Cross-Platform IDE for PHP by JetBrains.",
    "extract_to": "IDE",
    "hash": "fdcff3029c5d58cb34e4065a06f582c0a30f5c06ce7c4906d58a25bc8ddce76a",
    "homepage": "https://www.jetbrains.com/phpstorm/",
    "installer": {
        "script": [
            "& \"$bucketsdir\\extras\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse"
        ]
    },
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "IDE\\bin\\phpstorm.exe.vmoptions",
        "IDE\\bin\\phpstorm64.exe.vmoptions",
        "profile"
    ],
    "url": "https://download.jetbrains.com/webide/PhpStorm-2022.2.2.exe#/dl.7z",
    "version": "2022.2.2-222.4167.33"
}
