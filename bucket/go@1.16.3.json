{
    "version": "1.19.4",
    "homepage": "https://golang.org",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "post_install": [
        "",
        "$default_dirs = @( $env:GOPATH, \"$env:USERPROFILE\\go\", \"$env:USERPROFILE\\.go\" )",
        "$val = $( @($default_dirs |% { get-item $_ -ea silent }) + $default_dirs[0] ) | select -first 1",
        "ensure_in_path \"$val\\bin\" $global",
        ""
    ],
    "bin": [
        "bin/go.exe",
        "bin/gofmt.exe"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.19.4.windows-amd64.zip",
            "hash": "ada490e188bfb57c7388da7c5eba7565390992b6496204d30e710d37755956b0"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.19.4.windows-386.zip",
            "hash": "51d8d895deb9883aa2daa291572f483fead69f577bf4e7cf8381c8001e37778e"
        }
    },
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\."
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
