 "autoupdate": {
  "architecture": {
   "32bit": {
    "hash": {
     "regex": "$sha512\\s+$basename",
     "url": "https://cygwin.com/sha512.sum"
    },
    "url": "https://cygwin.com/setup-x86.exe#/cygwin-setup.exe"
   },
   "64bit": {
    "hash": {
     "regex": "$sha512\\s+$basename",
