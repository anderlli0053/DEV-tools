{
    "version": "1.0",
    "description": "Hexdump and byte2hex are command-line utilities for hexadecimal operations.",
    "homepage": "https://www.fileformat.info/tool/hexdump/index.htm",
    "license": "Freeware",
    "url": [
        "https://www.fileformat.info/tool/hexdump/hexdump.exe",
        "https://www.fileformat.info/tool/byte2hex/byte2hex.exe"
    ],
    "hash": [
        "5b7fd475f9d1f545a00d5f69829d368ba84c1e4a0eac3206c2389e4582898b85",
        "10548c673ae60dd4cc341acc77d96dab22e66313069f44d84970af1be2e35bac"
    ],
    "bin": [
        ["hexdump.exe", "hex2dump"],
        "byte2hex.exe"
    ],
    "notes": [
        "HEXDUMP USAGE:",
        "  hex2dump [options] <input file> [<output file>]",
        "",
        "  If no output file is specified, hex2dump writes to stdout.",
        "  Options:",
        "    -b   binary output (instead of hex)",
        "    -c   character output (instead of hex)",
        "    -C   character+hex output (like 'hexdump -C')",
        "    -d   decimal output (instead of hex)",
        "    -o   octal output (instead of hex)",
        "    -u   unsigned decimal output",
        "    -8   8-bit quantities (default)",
        "    -16  16-bit quantities",
        "    -32  32-bit quantities",
        "    -64  64-bit quantities",
        "    -l<len> number of bytes to dump (default is all)",
        "    -s<offset> start offset (in decimal)",
        "    -S<offset> start offset (in hex, e.g., -S0x100)",
        "",
        "  Examples:",
        "    hex2dump file.bin",
        "    hex2dump -C file.bin",
        "    hex2dump -8 -l256 -s0x100 file.bin out.txt",
        "",
        "BYTE2HEX USAGE:",
        "  byte2hex.exe [options] <input file> <output file>",
        "",
        "  Options:",
        "    -?   show this help message",
        "    -a   ASCII output (default is hex)",
        "    -b   binary output",
        "    -d   decimal output",
        "    -h   hex output (default)",
        "    -o   octal output",
        "    -u   unsigned decimal output",
        "    -8   8-bit quantities (default)",
        "    -16  16-bit quantities",
        "    -32  32-bit quantities",
        "    -64  64-bit quantities",
        "    -l<len> number of bytes to convert (default is all)",
        "    -s<offset> start offset (in decimal)",
        "    -S<offset> start offset (in hex, e.g., -S0x100)",
        "",
        "  Examples:",
        "    byte2hex.exe input.bin output.txt",
        "    byte2hex.exe -b input.bin output.txt",
        "    byte2hex.exe -16 -l512 -s0x200 input.bin output.txt"
    ]
}