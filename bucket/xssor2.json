{
  "version": "master",
  "description": "XSS'OR - Hack with JavaScript.(Scoop bucket by arch3rpro)",
  "homepage": "https://github.com/evilcos/xssor2",
  "license": "BSD-2-Clause",
  "suggest": "python2.7",
  "url": "https://ghproxy.com/https://github.com/evilcos/xssor2/archive/refs/heads/master.zip",
  "pre_install": "Set-Content \"$dir\\xssor2.bat\" '@pushd %~dp0\npython2 manage.py runserver 0.0.0.0:8000 %*\n@popd' -Encoding Ascii",
  "post_install": [
    "Move-Item -Path \"$dir\\xssor2-master\\*\" -Destination \"$dir\\\"",
    "Remove-Item \"$dir\\xssor2-master\" -Force -Recurse",
    "pip2 install -r \"$dir\\requirement.txt\""
  ],
  "bin": "xssor2.bat"
}
