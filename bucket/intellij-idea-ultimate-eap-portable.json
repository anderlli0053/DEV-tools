{
    "architecture": {
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "JetBrains\\IntelliJ IDEA Ultimate (Early Access Program)"
                ]
            ]
        },
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "JetBrains\\IntelliJ IDEA Ultimate (Early Access Program)"
                ]
            ]
        }
    },
    "autoupdate": {
        "hash": {
            "url": "$url.sha256"
        },
        "url": "https://download.jetbrains.com/idea/ideaIU-$preReleaseVersion.win.zip"
    },
    "checkver": {
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}",
        "url": "https://data.services.jetbrains.com/products/releases?code=IIU&latest=true&platform=zip&type=eap"
    },
    "description": "IntelliJ IDEA Ultimate Early Access Program. Cross-Platform Java IDE for professional developers by JetBrains.",
    "extract_to": "IDE",
    "hash": [
        "a05fb890088c35154403bb0a01b753fc1bfb3e53c427321af38b570601093e68",
        "2255673952a7909596b8783f6e913d2301f91f0def0275fcfbc6371b0ca021f8"
    ],
    "homepage": "https://www.jetbrains.com/idea/",
    "installer": {
        "script": [
            "& \"$dir\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\", \"$dir\\portable.ps1\" -Force -Recurse"
        ]
    },
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/legal/agreements/user_eap.html"
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "url": [
        "https://download.jetbrains.com/idea/ideaIU-231.6471.13.win.zip",
        "https://raw.githubusercontent.com/Ash258/Scoop-JetBrains/main/scripts/portable.ps1"
    ],
    "version": "2023.1-231.6471.13"
}
