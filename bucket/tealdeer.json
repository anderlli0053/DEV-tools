{
 "architecture": {
  "64bit": {
   "hash": "abf2708b4932bc67bbee7c194d482146bad2fc880ee9d54dd0892524601bad31",
   "url": "https://github.com/dbrgn/tealdeer/releases/download/v1.6.1/tealdeer-windows-x86_64-msvc.exe#/tldr.exe"
  }
 },
 "autoupdate": {
  "architecture": {
   "64bit": {
    "hash": {
     "url": "$url.sha256"
    },
    "url": "https://github.com/dbrgn/tealdeer/releases/download/v$version/tealdeer-windows-x86_64-msvc.exe#/tldr.exe"
   }
  }
 },
 "bin": "tldr.exe",
 "checkver": {
  "github": "https://github.com/dbrgn/tealdeer"
 },
 "description": "A very fast implementation of tldr in Rust.",
 "env_set": {
  "TEALDEER_CONFIG_DIR": "$dir"
 },
 "homepage": "https://dbrgn.github.io/tealdeer",
 "license": "Apache-2.0|MIT",
 "persist": [
  "config.toml",
  "cache",
  "custom-pages"
 ],
 "pre_install": [
  "$file = 'config.toml'",
  "if (!(Test-Path \"$persist_dir\\$file\")) {",
  "   Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
  "   $CONT = @(",
  "       '[directories]'",
  "       \"cache_dir = `\"$persist_dir\\cache`\"\"",
  "       \"custom_pages_dir = `\"$persist_dir\\custom-pages`\"\"",
  "   ) -replace '\\\\', '\\\\'",
  "   Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding ASCII",
  "}"
 ],
 "version": "1.6.1"
}