{
  "version": "2.9.3",
  "description": "The Haskell Tool Stack",
  "homepage": "https://www.haskellstack.org",
  "license": "BSD-3-Clause",
  "notes": [
    "Add \"local-bin-path: <<BIN_PATH>>\" to \"$(stack path --stack-root)\"to reset `stack` binary destination path.",
    "For `hlint`, use `stack install hlint`."
  ],
  "architecture": {
    "64bit": {
      "url": "https://ghproxy.com/https://github.com/commercialhaskell/stack/releases/download/v2.9.3/stack-2.9.3-windows-x86_64.zip",
      "hash": "9403e374429aaab4d8843d9ed1bb616f7c94bea81fee0f594412b8bbb6b98970"
    }
  },
  "bin": "stack.exe",
  "installer": {
    "script": "add_first_in_path \"$env:APPDATA\\local\\bin\" $global"
  },
  "uninstaller": {
    "script": "remove_from_path \"$env:APPDATA\\local\\bin\" $global"
  },
  "checkver": {
    "github": "https://github.com/commercialhaskell/stack"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ghproxy.com/https://github.com/commercialhaskell/stack/releases/download/v$version/stack-$version-windows-x86_64.zip"
      }
    },
    "hash": {
      "url": "$url.sha256"
    }
  }
}
