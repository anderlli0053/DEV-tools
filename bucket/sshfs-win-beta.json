{
  "##": "Renaming .msi to .msi_ so that it will not be automatically extracted by Scoop.",
  "homepage": "https://github.com/billziss-gh/sshfs-win",
  "description": "SSHFS for Windows.",
  "version": "3.7.21011",
  "license": "GPL-2.0-or-later",
  "architecture": {
    "64bit": {
      "url": "https://ghproxy.com/https://github.com/billziss-gh/sshfs-win/releases/download/v3.7.21011/sshfs-win-3.7.21011-x64.msi#/setup.msi_",
      "hash": "76080d7bfb1ba0a807f8874d07388bec4bc30893f2da511d5cb7a16d4490f7d1"
    },
    "32bit": {
      "url": "https://ghproxy.com/https://github.com/billziss-gh/sshfs-win/releases/download/v3.7.21011/sshfs-win-3.7.21011-x86.msi#/setup.msi_",
      "hash": "f74e399a44b54b447f7e8ed4f012ba1932c5f90be71a6942ea304841eab610b0"
    }
  },
  "depends": "winfsp",
  "bin": [
    "bin\\sshfs.exe",
    "bin\\sshfs-win.exe"
  ],
  "installer": {
    "script": "Invoke-ExternalCommand msiexec -ArgumentList \"/i $dir\\setup.msi_ INSTALLDIR=$dir ARPSYSTEMCOMPONENT=0 /qn\" -RunAs"
  },
  "uninstaller": {
    "script": "Invoke-ExternalCommand msiexec -ArgumentList \"/x $dir\\setup.msi_ /qn\" -RunAs"
  },
  "checkver": {
    "url": "https://github.com/billziss-gh/sshfs-win/releases",
    "re": "v(?<short>[\\d.]+)/sshfs-win-(?<version>[\\d.]+).*\\.msi"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ghproxy.com/https://github.com/billziss-gh/sshfs-win/releases/download/v$matchShort/sshfs-win-$matchVersion-x64.msi#/setup.msi_"
      },
      "32bit": {
        "url": "https://ghproxy.com/https://github.com/billziss-gh/sshfs-win/releases/download/v$matchShort/sshfs-win-$matchVersion-x86.msi#/setup.msi_"
      }
    }
  }
}
