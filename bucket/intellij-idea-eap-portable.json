{
    "architecture": {
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "JetBrains\\IntelliJ IDEA Community (Early Access Program)"
                ]
            ]
        },
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "JetBrains\\IntelliJ IDEA Community (Early Access Program)"
                ]
            ]
        }
    },
    "autoupdate": {
        "hash": {
            "url": "$url.sha256"
        },
        "url": "https://download.jetbrains.com/idea/ideaIC-$preReleaseVersion.win.zip"
    },
    "checkver": {
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}",
        "url": "https://data.services.jetbrains.com/products/releases?code=IIC&latest=true&platform=zip&type=eap"
    },
    "description": "IntelliJ IDEA Community Early Access Program. Cross-Platform Java IDE for professional developers by JetBrains.",
    "extract_to": "IDE",
    "hash": [
        "f7569189679837561e40e0aac3a9ec9bd3398393ffa72d96aa29f248f807b42a",
        "2255673952a7909596b8783f6e913d2301f91f0def0275fcfbc6371b0ca021f8"
    ],
    "homepage": "https://www.jetbrains.com/idea/",
    "installer": {
        "script": [
            "& \"$dir\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\", \"$dir\\portable.ps1\" -Force -Recurse"
        ]
    },
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://sales.jetbrains.com/hc/en-gb/articles/115001015290-Where-can-I-find-the-EULA-End-User-License-Agreement-"
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "url": [
        "https://download.jetbrains.com/idea/ideaIC-231.6890.12.win.zip",
        "https://raw.githubusercontent.com/Ash258/Scoop-JetBrains/main/scripts/portable.ps1"
