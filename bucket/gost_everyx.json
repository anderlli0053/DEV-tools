{
  "homepage": "https://github.com/ginuerzh/gost",
  "description": "GO Simple Tunnel - a simple tunnel written in golang",
  "version": "2.11.1",
  "persist": "config.json",
  "pre_install": [
    "if (!(Test-Path \"$persist_dir\\config.json\")) {",
    "    New-Item -Force -Path \"$dir\" -Name \"config.json\" | Out-Null",
    "}"
  ],
  "architecture": {
    "64bit": {
      "bin": [
        [
          "gost-windows-amd64.exe",
          "gostc",
          "-C $dir/config.json"
        ],
        [
          "gost-windows-amd64.exe",
          "gost"
        ]
      ],
      "url": "https://ghproxy.com/https://github.com/ginuerzh/gost/releases/download/v2.11.1/gost-windows-amd64-2.11.1.zip",
      "hash": "d5d56d37b8cddf6e079755b9fb76ca1bfa24ee608bb8abfbc903048aad83147f"
    },
    "32bit": {
      "bin": [
        [
          "gost-windows-386.exe",
          "gostc",
          "-C $dir/config.json"
        ],
        [
          "gost-windows-386.exe",
          "gost"
        ]
      ],
      "url": "https://ghproxy.com/https://github.com/ginuerzh/gost/releases/download/v2.11.1/gost-windows-386-2.11.1.zip",
      "hash": "6f960d1cd41708bb12b9461cac13543ab1b75bc8b228c34a8802ebd6a195bbb3"
    }
  },
  "checkver": "github",
  "autoupdate": {
    "architecture": {
      "64bit": {
