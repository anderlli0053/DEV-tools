{
  "$schema": "https://ghproxy.com/https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
  "homepage": "https://github.com/BenjaminMichaelis/ScoopBucket",
  "description": "Setups my personal scoop bucket.",
  "notes": "Of course, this script is placed in the BenjaminMichaelis bucket so you it presents a chicken and egg problem.",
  "version": "1.0.0",
  "installer": {
    "script": [
      "scoop bucket add BenjaminMichaelis https://github.com/BenjaminMichaelis/ScoopBucket.git"
    ]
  },
  "uninstaller": {
    "script": [
      "scoop bucket rm BenjaminMichaelis"
    ]
  },
  "url": "https://ghproxy.com/https://raw.githubusercontent.com/BenjaminMichaelis/ScoopBucket/master/blank"
}
