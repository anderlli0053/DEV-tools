        "Start-Process \"$dir\\IE_Install.exe\" -Wait -ArgumentList @('/SP-', '/VERYSILENT', '/PORTABLE=1', \"/DIR=$dir\"); Remove-Item \"$dir\\IE_Install.exe\"",
        "Copy-Item \"$persist_dir\\Config.ini\" \"$dir\\Config.ini\" -ErrorAction 'SilentlyContinue' -Force"
    ],
