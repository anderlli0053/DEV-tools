            "hash": "cece52a276629e838291029193378f3c2483a27afd1a8e654b7e82d81d6cf749"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/ac6qxeyf56yiv7u7curjssx7rzta_123.0.6273.0/123.0.6273.0_chrome_installer.exe#/dl.7z",
