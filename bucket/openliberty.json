    "license": "EPL-1.0",
    "notes": "server.bat is aliased to wlp, client.bat is aliased to wlp-client",
    "persist": "usr",
    "suggest": {
        "JDK": "java/openjdk"
    },
    "url": "https://public.dhe.ibm.com/ibmdl/export/pub/software/openliberty/runtime/release/22.0.0.9/openliberty-22.0.0.9.zip",
