{
  "homepage": "https://github.com/liuzijing97/scoop-completion/",
  "description": "Scoop tab completion, work with powershell",
  "version": "nightly",
  "license": "MIT",
  "url": "https://ghproxy.com/https://github.com/liuzijing97/scoop-completion/archive/master.zip",
  "extract_dir": "scoop-completion-master/src",
  "psmodule": {
    "name": "scoop-completion"
  },
  "post_install": "Import-Module $env:USERPROFILE\\scoop\\modules\\scoop-completion",
  "notes": [
    "To enable completion autoload, you need to add scoop-completion module to PowerShell $PROFILE by running:",
    "Add-Content -Path $Profile -Value \"`nImport-Module $env:USERPROFILE\\scoop\\modules\\scoop-completion\""
  ]
}
