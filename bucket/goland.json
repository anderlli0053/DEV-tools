   "shortcuts": [
    [
     "IDE\\bin\\goland.exe",
     "JetBrains\\GoLand"
    ]
   ]
  },
  "64bit": {
   "bin": [
    [
     "IDE\\bin\\goland64.exe",
     "goland"
    ]
   ],
   "shortcuts": [
    [
     "IDE\\bin\\goland64.exe",
     "JetBrains\\GoLand"
    ]
   ]
