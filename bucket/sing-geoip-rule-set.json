        "regex": "(?<date>\\d+) (?<full>(?<short>[a-f0-9]{7})[a-f0-9]+)",
        "replace": "${date}-${short}"
    },
    "autoupdate": {
        "url": "https://github.com/SagerNet/sing-geoip/archive/$matchFull.zip",
        "extract_dir": "sing-geoip-$matchFull"
    }
