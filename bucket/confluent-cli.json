{
    "version": "4.45.1",
    "description": "The Confluent CLI lets you manage your Confluent Cloud and Confluent Platform deployments, right from the terminal.",
    "homepage": "https://docs.confluent.io/confluent-cli/current/overview.html",
    "license": {
        "identifier": "Proprietary",
        "url": "https://raw.githubusercontent.com/confluentinc/cli/refs/heads/main/LICENSE"
    },
    "notes": [
        "After installation, run 'confluent' to get started.",
        "You may need to run 'confluent login' to authenticate with your Confluent account."
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/confluentinc/cli/releases/download/v4.45.1/confluent_4.45.1_windows_amd64.zip",
            "hash": "sha256:917d94f80c0f06e335f5b9607049b63623ebcccad045ea33f7769aaff5e5e93d"
        }
    },
    "pre_install": [
        
        "$innerFolder = Get-ChildItem \"$dir\" -Directory | Where-Object { $_.Name -match 'confluent' } | Select-Object -First 1",
        "if ($innerFolder) {",
        "    Get-ChildItem \"$($innerFolder.FullName)\" | ForEach-Object {",
        "        Move-Item \"$($_.FullName)\" \"$dir\"",
        "    }",
        "    Remove-Item \"$($innerFolder.FullName)\"",
        "}"
    ],
    "bin": "confluent.exe",
    "checkver": {
        "github": "https://github.com/confluentinc/cli",
        "regex": "releases/tag/v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/confluentinc/cli/releases/download/v$version/confluent_$version_windows_amd64.zip"
            }
        },
        "hash": {
            "mode": "download",
            "url": "https://github.com/confluentinc/cli/releases/download/v$version/confluent_checksums.txt",
            "find": "([a-fA-F0-9]{64})\\s+confluent_$version_windows_amd64\\.zip"
        }
    }
}