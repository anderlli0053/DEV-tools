{
  "version": "9.4.1",
  "description": "An advanced, purely functional programming language.",
  "homepage": "https://www.haskell.org",
  "license": "BSD-3-Clause",
  "notes": [
    "For Haskell `cabal`, please install 'haskell-cabal'.",
    "For Haskell `stack`, please install 'haskell-stack'."
  ],
  "suggest": {
    "Cabel": "haskell-cabal",
    "Stack": "haskell-stack"
  },
  "architecture": {
    "64bit": {
      "url": "https://ghproxy.com/https://github.com/commercialhaskell/ghc/releases/download/ghc-9.4.1-release/ghc-9.4.1-x86_64-unknown-mingw32.tar.xz",
      "hash": "fdc2d78b8a978e712a4edcc1628e36a36be736d8202107d2d61999ac7a9dc5d0",
      "extract_dir": "ghc-9.4.1-x86_64-unknown-mingw32"
    }
  },
  "bin": [
    "bin\\ghc.exe",
    "bin\\ghci.exe",
    "bin\\ghc-pkg.exe",
    "bin\\haddock.exe",
    "bin\\hp2ps.exe",
    "bin\\hpc.exe",
    "bin\\hsc2hs.exe",
    "bin\\runghc.exe",
    "bin\\touchy.exe",
    "bin\\unlit.exe"
  ],
  "env_add_path": "lib\\bin",
  "checkver": {
    "url": "https://api.github.com/repos/commercialhaskell/ghc/tags",
    "jsonpath": "$..name",
    "regex": "ghc-([\\d\\.]+)-release"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ghproxy.com/https://github.com/commercialhaskell/ghc/releases/download/ghc-$version-release/ghc-$version-x86_64-unknown-mingw32.tar.xz",
        "extract_dir": "ghc-$version-x86_64-unknown-mingw32"
      }
    }
  }
}
