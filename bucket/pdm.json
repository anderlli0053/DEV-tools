    "installer": {
        "script": [
            "python -m venv \"$dir\\venv\"",
            "& \"$dir\\venv\\Scripts\\python.exe\" -m pip install \"$dir\\$fname\""
        ]
    },
    "license": "MIT",
    "post_install": "& \"$dir\\venv\\Scripts\\pdm.exe\" --pep582",
