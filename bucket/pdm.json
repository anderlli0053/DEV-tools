{
    "autoupdate": {
        "url": "https://files.pythonhosted.org/$match1"
    },
    "bin": "venv\\Scripts\\pdm.exe",
    "checkver": {
        "re": "https://files.pythonhosted.org/(packages/.*/pdm-(?<version>[\\d.]+)-.*\\.whl)",
        "replace": "${version}",
